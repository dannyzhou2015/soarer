	EXTRA_F1	ESC
	EXTRA_F2    MEDIA_PLAY_PAUSE
	EXTRA_F3	MEDIA_VOLUME_DOWN
	EXTRA_F4	MEDIA_VOLUME_UP
	EXTRA_F5	MEDIA_MUTE
	EXTRA_F6	MEDIA_WWW_HOME
	EXTRA_F7	MEDIA_WWW_BACK
	EXTRA_F8	MEDIA_WWW_FORWARD
	EXTRA_F9	LGUI
	EXTRA_F10	MEDIA_WWW_SEARCH
