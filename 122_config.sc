	# block 1
	F13		    MEDIA_MY_COMPUTER
	F14			MEDIA_MEDIA_SELECT
	F15			MEDIA_MAIL
	F16			MEDIA_CALCULATOR
	# block 2
	F17			SCROLL_LOCK
	F18			MEDIA_PREV_TRACK
	F19			MEDIA_NEXT_TRACK
